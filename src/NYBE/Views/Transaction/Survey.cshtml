@model NYBE.Models.TransactionViewModels.SurveyViewModel

@{
    ViewData["Title"] = "Confirmation Survey";
}

<h2 class="text-center">@ViewData["Title"]</h2>

<div class="row">
    <div class="col-sm-10 col-sm-offset-1">
        <form asp-controller="Transaction" asp-action="Survey" method="post" class="form-horizontal">
            <hr />

            <hr />
            <div class="col-sm-8 col-sm-offset-5">
                <button type="button" id="back" onclick="history.go(-1)" class="btn btn-danger">Cancel</button>
                <button type="submit" id="back" onclick="history.go(-1)" class="btn btn-success">Submit</button>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
